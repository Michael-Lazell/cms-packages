<?xml version="1.0" encoding="UTF-8"?>
<datasources sessioncontext="true">
    <datasource result-element="menu">
        <methodname>getMenuBranch</methodname>
        <parameters>
            <parameter name="menuItemKey" type="int">${portal.pageKey}</parameter>
            <parameter name="includeTopLevel" type="boolean">true</parameter>
            <parameter name="startLevel" type="int">0</parameter>
            <parameter name="levels" type="int">0</parameter>
        </parameters>
    </datasource>
    <datasource>
        <methodname>getPreferences</methodname>
        <parameters>
            <parameter name="scope" type="string">*</parameter>
            <parameter name="wildCardkey" type="string">*</parameter>
        </parameters>
    </datasource>
    <datasource>
        <methodname>getUserstore</methodname>
        <parameters>
            <parameter name="userstore" type="string"/>
        </parameters>
    </datasource>
    <datasource>
        <methodname>getTimeZones</methodname>
        <parameters/>
    </datasource>
    <datasource>
        <methodname>getLocales</methodname>
        <parameters/>
    </datasource>
    <datasource>
        <methodname>getCountries</methodname>
        <parameters>
            <parameter name="countryCodes" type="string[]"/>
            <parameter name="includeRegions" type="boolean">true</parameter>
        </parameters>
    </datasource>
</datasources>